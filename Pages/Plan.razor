@page "/plan"

<PageTitle>Elegir Plan</PageTitle>

<h1>Planes</h1>

<p role="status">Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Seleccionar</button>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}
